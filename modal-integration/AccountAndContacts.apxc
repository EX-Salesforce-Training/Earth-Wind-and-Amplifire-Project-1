// @desc: gathers account and contact info

public class AccountAndContacts {
    public List<Account> accs {get;set;}
    
    
    public AccountAndContacts() {
        accs = [SELECT Name, Phone, (SELECT FirstName, LastName, Phone, Email FROM Contacts) FROM Account];
    }
}